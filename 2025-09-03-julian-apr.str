$: n(saw.rev().range(0, 14)
     .add(saw.range(0, 7*2).slow(4))
     .add(rand.range(-2,0))
     .seg("16 8 4")
     .sometimes(ply(2))
     .rib(0, 8)
    )
  .scale("<c:major>/8").sound("pulse")
  .gain(sine.range(.6,.8).fast(2))
  .lpenv(3)
  .lpf(200)

$: s("sbd*4").gain(.5)

// $: s("[- hh]*8").gain("[.2 .4]*4")

